{
  "name": "mycoriza-cli",
  "version": "0.0.2-cli",
  "description": "A simple and zero-opinion typescript starter template for building cross-platform command line applications.",
  "author": "Tiran Wijesekara",
  "private": false,
  "keywords": [
    "typescript",
    "cli",
    "starter",
    "code-generator",
    "react"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": {
    "mycoriza": "bin/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*",
    "!dist/**/*.test.js"
  ],
  "scripts": {
    "dev": "./dist/cli.js enhance",
    "clean": "rimraf ./dist/ ./exec/",
    "build": "npm run clean && tsc",
    "test": "npm run build && ava --verbose",
    "bundle": "npm run build && pkg . --out-dir ./exec/"
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "pkg": "^4.4.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/handlebars":         "^4.1.0",
    "@types/inquirer": "^7.3.3",
    "@types/inquirer-fuzzy-path": "^2.3.1",
    "@types/node-emoji": "^1.8.1",
    "@types/rimraf": "^3.0.2",
    "bottom-bar": "^1.0.2",
    "camelcase": "^6.2.0",
    "chalk": "^4.1.2",
    "cli-spinners": "^2.6.0",
    "commander": "^8.0.0",
    "date-fns": "^2.23.0",
    "git-user-name": "^2.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^8.1.2",
    "inquirer-fuzzy-path": "^2.3.0",
    "js-yaml": "^4.1.0",
    "jsonschema7": "^0.8.0",
    "node-emoji": "^1.11.0",
    "openapi-types": "^9.1.0",
    "openapi-typescript-codegen": "^0.9.3",
    "xml-beautifier": "^0.5.0"
  }
}
